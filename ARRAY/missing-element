#include<bits/stdc++.h>
using namespace std;

int main()
{
    vector<int>a;
    int n,ans,m,i,tot=0,sumarray=0;
    cin>>n;
    for(i=0;i<n-1;i++)
    {
        cin>>m;
        a.push_back(m);
        sumarray+=m;
    }
    
    tot = n * (n + 1 ) / 2;
    ans=tot - sumarray;
    cout<<ans;
    return 0;
}
